<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Here are all your skills</title>
  </head>
  <body>
    <h1>Here are all your skills</h1>
    <ul>
      <% skills.forEach(function(s) { %>
      <li>
        <strong>Skill:</strong> <%= s.skill%> - <strong>Level:</strong> <%=
        s.level %>
      </li>
      <% }) %>
    </ul>
  </body>
</html>
